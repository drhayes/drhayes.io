---
layout: default.njk
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - article
    - blog
    - stub
    - writing
  addAllPagesToCollections: false
permalink: /tags/{{ tag | slugify }}/
eleventyComputed:
  title: "#{{ tag }}"
changefreq: weekly
eleventyExcludeFromCollections: true
---
{% set taglist = collections[tag] %}
{% set postList = taglist | reverse %}
<main class="tagged-things">
  {% include "post-list.njk" %}
</main>
